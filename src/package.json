{
  "app_name": "pult",
  "main": "app.html",
  "version": "0.1.0",
  "author": "Serg <s.kanunnikov@it-tula.ru>",
  "chromium-args": "--disable-setuid-sandbox",
  "name": "nw-pult",
  "app-id": "com.pult.starter",
  "window": {
    "icon": "favicon.png",
    "kiosk": false,
    "show": true,
    "width": 980,
    "visible": true,
    "title": "Пульт оператора",
    "mac_icon": "favicon.png",
    "resizable": false,
    "exe_icon": "favicon.png",
    "always-on-top": false,
    "fullscreen": false,
    "frame": true,
    "height": 700,
    "show_in_taskbar": true,
    "transparent": false,
    "kiosk_emulation": false,
    "as_desktop": false,
    "toolbar": true
  },
  "webkit": {
    "plugin": true
  },
  "js-flags": "--harmony_proxies --harmony_collections",
  "dependencies": {
    "async": "^1.0.0",
    "auto-launch": "^0.1.18",
    "jfs": "^0.2.5",
    "object.observe": "^0.2.6",
    "observed": "^1.1.2",
    "request": "^2.56.0",
    "semver": "^4.3.5"
  },
  "manifestUrl": "https://raw.githubusercontent.com/Aluxian/nwjs-starter/master/src/package.json",
  "packages": {
    "osx64": "https://github.com/Aluxian/nwjs-starter/releases/download/v0.1.0/Starter.dmg",
    "win32": "https://github.com/Aluxian/nwjs-starter/releases/download/v0.1.0/StarterSetup.exe",
    "linux32": "https://github.com/Aluxian/nwjs-starter/releases/download/v0.1.0/Starter_linux32.deb",
    "linux64": "https://github.com/Aluxian/nwjs-starter/releases/download/v0.1.0/Starter_linux64.deb"
  }
}
